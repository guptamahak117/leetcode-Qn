class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int sum=0;
        int tot_sum=0;
         int n=nums.size();
        for(int i=0;i<n;i++){
            sum=sum+nums[i];
        }
        for(int j=0;j<=n;j++){
            tot_sum=tot_sum+j;
        }
        int diff=tot_sum-sum;
        return diff;
    }    
    
};
